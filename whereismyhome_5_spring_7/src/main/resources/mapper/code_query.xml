<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mycom.myapp.user.dao.CodeDao">   <!-- @Mapper annotation 있는 dao -->

	<select id="getCodeList"
		parameterType="string" resultType="com.mycom.myapp.user.dto.CodeDto">
		select code, code_name FROM code where group_code = #{groupCode}
	</select>

</mapper>