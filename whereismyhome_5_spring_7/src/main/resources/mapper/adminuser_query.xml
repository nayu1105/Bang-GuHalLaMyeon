<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycom.myapp.admin.user.dao.AdminUserDao">
<!-- userList -->
	<select id="userList" parameterType="com.mycom.myapp.admin.user.dto.AdminUserParamDto" resultType="com.mycom.myapp.admin.user.dto.AdminUserDto">
		 select user_seq, user_name, user_password, user_email, user_profile_image_url, user_register_date, user_clsf
		 from USERS
		 order by user_seq
		 limit #{limit} offset #{offset}
	</select>
	
<!-- userListCnt -->
	<select id="userListTotalCnt" resultType="int">
	select count(*) from USERS
	</select>
	

</mapper>